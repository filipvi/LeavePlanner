                        orientation: 'landscape',
                        exportOptions: {
                            columns: [0, 1, 2, 3, 4, 5]
                        },
                        customize: function(doc) {

                            doc['footer'] = (function(page, pages) {
                                return {
                                    columns: [
                                        'TEST',
                                        {
                                            alignment: 'right',
                                            text: ['Stranica ', { text: page.toString() }, ' od ', { text: pages.toString() }]
                                        }
                                    ],
                                    margin: [20, 20]
                                }
                            });
